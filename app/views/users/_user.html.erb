<% if user.is_staying %>
  <%= form_for( user, url: leave_room_user_path(user.room_id, user.id), remote: true) do |f| %>
    <%= avatar_imgsubmittag(user, class_name: "is_staying") %>
  <% end %>
<% else %>
  <%= form_for( user, url: enter_room_user_path(user.room_id, user.id), remote: true) do |f| %>
    <%= avatar_imgsubmittag(user, class_name: "not_staying") %>
  <% end %>
<% end %>